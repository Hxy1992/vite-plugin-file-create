{
  "name": "vite-plugin-file-create",
  "version": "0.0.1",
  "description": "一个vite插件, 在vite打包时创建文件并保存至输出目录",
	"type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "scripts": {
		"dev": "tsup --watch",
    "build": "tsup"
  },
  "keywords": [
    "vite",
    "vite-plugin"
  ],
  "license": "MIT",
  "homepage": "https://github.com/Hxy1992/vite-plugin-file-create",
  "sideEffects": false,
  "peerDependencies": {
    "vite": "^3.0.0 || ^4.0.0"
  },
  "devDependencies": {
		"@types/node": "^17.0.31",
		"@typescript-eslint/eslint-plugin": "^5.22.0",
		"@typescript-eslint/parser": "^5.22.0",
    "tsup": "^6.7.0",
		"typescript": "^4.5.4",
		"vite": "^3.2.5"
  },
  "dependencies": {
  }
}
